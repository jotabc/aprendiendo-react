  "devDependencies": {
    "@swc/core": "1.3.61",
    "@swc/cli": "0.1.62",
    "@testing-library/react": "14.0.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitejs/plugin-react-swc": "3.0.0",
    "@vitest/ui": "0.31.4",
    "happy-dom": "9.20.3",
    "vite": "4.3.2",
    "vitest": "0.31.4"
  }
